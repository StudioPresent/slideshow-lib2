{
  "name": "studiopresent/slideshow",
    "description": "Drupal 8 views slideshow and libraries",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "David Malatenszki",
            "email": "malatenszki.david@gmail.com"
        }
    ],
  "require": {
    "composer/installers": "^1.0.2",
    "jquery/jquery.cycle": "^3.0",
    "jquery/jquery.hoverIntent": "^1.8",
    "json/json2": "dev-master"
  },
  "minimum-stability": "dev",
  "extra": {
    "installer-paths": {
      "public_html/libraries/{$name}": [
        "type:drupal-library"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ]
    }
  }
}
