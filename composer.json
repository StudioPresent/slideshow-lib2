{
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "jquery.cycle": {
      "type": "package",
      "package": {
        "name": "jquery/jquery.cycle",
        "version": "3.0.3",
        "type": "drupal-library",
        "dist": {
          "url": "https://raw.githubusercontent.com/malsup/cycle/master/jquery.cycle.all.js",
          "type": "file"
        }
      }
    }
  },
  "require": {
    "composer/installers": "^1.0.2",
    "jquery/jquery.cycle": "*",
    "jquery/jquery.hoverIntent": "*",
    "json/json2": "*",
    "drupal/views_slideshow": "^4.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "public_html/libraries/{$name}": [
        "type:drupal-library"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ]
    }
  }
}
